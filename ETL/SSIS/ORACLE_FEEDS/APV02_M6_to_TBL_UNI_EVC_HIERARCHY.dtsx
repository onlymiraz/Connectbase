              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT 
UED.STATE,
UED.SERVICE_TYPE_CODE,
CIR.CIRCUIT_DESIGN_ID AS EVC_DSGN_ID,
CIR.LAST_MODIFIED_DATE AS LAST_MODIFIED_DATE,
DECODE (CIR.STATUS,
  '1', '1 - Pending', 
  '2', '2 - Assigned',
  '3', '3 - In Progress',
  '4', '4 - CLR Issued',
  '5', '5 - DLR Issued',
  '6', '6 - In Service',
  '7', '7 - Pending Disconnect',
  '8', '8 - Disconnected',
  '9', '9 - Problem',
  'A', 'A - Cancelled',
  'P', 'P - Planned',
  null) as STATUS_DESC,
REGEXP_REPLACE(UED.VIR_CIRCUIT,'[^a-zA-Z0-9'']','') AS CLEAN_EVC_ID,
UED.VIR_CIRCUIT AS EVC_CIRCUIT,
(select max(ccv.ca_value) keep (dense_rank last order by ccv.conn_ca_value_id) * 
  (CASE WHEN max(ccv.ca_value_uom) keep (dense_rank last order by ccv.conn_ca_value_id) = 'G' THEN 1000 ELSE 1 END)
  from conn_ca_value ccv  where CIR.CIRCUIT_DESIGN_ID = ccv.circuit_design_id and  ccv.ca_value_label = 'CIR') as EVC_SPEED,
COUNT(*) OVER (PARTITION BY UED.VIR_CIRCUIT) AS EVC_RELATED_TO_N,
REGEXP_REPLACE(UED.EXCHANGE_CARRIER_CIRCUIT_ID,'[^a-zA-Z0-9'']','') AS CLEAN_UNI_ID,
UED.EXCHANGE_CARRIER_CIRCUIT_ID AS UNI_CIRCUIT,
CASE WHEN CUD.UNI_OR_NNI = 435 THEN 'UNI'
     WHEN CUD.UNI_OR_NNI = 436 THEN 'NNI' ELSE NULL END AS UNI_NNI,
COUNT(*) OVER (PARTITION BY EXCHANGE_CARRIER_CIRCUIT_ID) AS UNI_RELATED_TO_N,
rank() over (partition by EXCHANGE_CARRIER_CIRCUIT_ID order by ued.vir_circuit) as rank,
DLR_ACNA,
am.primary_carrier_name
-- FROM TEAM_OSS.MV_UNI_EVC UE
FROM
(SELECT CIRCUIT_DESIGN_ID, LAST_MODIFIED_DATE, STATUS FROM CIRCUIT WHERE (SUBSTR(EXCHANGE_CARRIER_CIRCUIT_ID,3,3) = '/VL' OR NETWORK_CHANNEL_SERVICE_CODE = 'VL')) CIR
LEFT OUTER JOIN
(SELECT * FROM TEAM_OSS.MV_UNI_EVC_DETAIL) UED ON CIR.CIRCUIT_DESIGN_ID = UED.VIRT_CD
LEFT OUTER JOIN ASR_OM.ASR_ACCOUNT_MANAGER AM ON ued.dlr_acna = am.secondary_id
LEFT OUTER JOIN CIRCUIT_USER_DATA CUD ON UED.circuit_design_id = CUD.circuit_design_id
--WHERE (UED.STATUS = '6' OR UED.STATUS IS NULL)
--AND UED.VIRT_STATUS = '6'
WHERE DLR_ACNA NOT IN ('BLI','CQV','CZJ','CZX','EPX','FLR','FLX','FTR','GOV','SUV','ZWV','ZZZ',
                      'FIS','W05','ANV','FCA','GVN','ZAP','GSW','WBY','CUS','VZN',
                      'AF1','CXN','EXC','FBA','T05','WWW','ZTK') -- EXCLUDE RETAIL/AFFILIATE ACNAs
and  CIR.LAST_MODIFIED_DATE &gt; date '2023-04-14' 
--AND DLR_ACNA NOT IN ('BLI','CQV','CZJ','CZX','EPX','FLR','FLX','FTR','GOV','SUV','ZWV','ZZZ',
  --                    'FIS','W05','ANV','FCA','ALN','GVN','ZAP','GSW','GOP','WBY','CCQ','RTC','CUS','VZN')
/*and REGEXP_REPLACE(UED.EXCHANGE_CARRIER_CIRCUIT_ID,'[^a-zA-Z0-9'']','') in (
'87KRGN025150CZUC',
'87KRGN101594CZUC',
'87KRGN025140CZUC',
'50KRGN880100FVNC'
)
*/

ORDER BY CLEAN_EVC_ID, CLEAN_UNI_ID</property>

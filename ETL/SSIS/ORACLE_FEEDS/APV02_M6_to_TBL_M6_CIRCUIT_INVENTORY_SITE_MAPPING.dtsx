              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">WITH LatestCircuit AS (
  SELECT *
  FROM circuit
  WHERE last_modified_date = (
    SELECT MAX(last_modified_date)
    FROM circuit cir2
    WHERE Regexp_replace(cir2.exchange_carrier_circuit_id, '[^a-zA-Z0-9]', '') = Regexp_replace(circuit.exchange_carrier_circuit_id, '[^a-zA-Z0-9]', '')
  )
)
,maxCompletionDate AS (
  SELECT 
    DOCUMENT_NUMBER, 
    TASK_TYPE, 
    MAX(ACTUAL_COMPLETION_DATE) as DD_COMPLETION_DATE
  FROM task
  where TASK_TYPE = 'DD'
  GROUP BY DOCUMENT_NUMBER, TASK_TYPE
)
--,finalCircuits as (
SELECT DISTINCT 
  cir.EXCHANGE_CARRIER_CIRCUIT_ID AS EXCHANGE_CARRIER_CIRCUIT_ID,
  Regexp_replace(cir.exchange_carrier_circuit_id, '[^a-zA-Z0-9]', '') AS CLEAN_ID,
  COALESCE(asr.CKR, asr2.ckr) AS SITE_ID,
  cir.STATUS AS STATUS,
  cir.SERVICE_TYPE_CODE AS SERVICE_TYPE_CODE,
  sr.ACNA as ACNA,
  case when SUBSTR(TRUNC(asrdtl.USERDATA_ACCEPTANCE_DATE),8, 2) in ('30','31','33','26', '40','43','50','60','70','80') THEN TO_DATE('01-JAN-1900', 'DD-MON-YYYY') 
  else TRUNC(asrdtl.USERDATA_ACCEPTANCE_DATE) end as IN_SERVICE_DATE,
  dd.DD_COMPLETION_DATE as DD_COMPLETION_DATE
FROM LatestCircuit cir
LEFT JOIN access_billing_circuit_data abcd
  ON Regexp_replace(cir.exchange_carrier_circuit_id, '[^a-zA-Z0-9]', '') = 
     Regexp_replace(abcd.exchange_carrier_circuit_id, '[^a-zA-Z0-9]', '')
LEFT JOIN access_service_request asr
  ON abcd.DOCUMENT_NUMBER = asr.DOCUMENT_NUMBER
LEFT JOIN maxCompletionDate dd 
  ON asr.DOCUMENT_NUMBER = dd.DOCUMENT_NUMBER
LEFT JOIN data_ext.asr_detail asrdtl
  ON asr.DOCUMENT_NUMBER = asrdtl.DOCUMENT_NUMBER
left JOIN  serv_req sr
  ON asr.DOCUMENT_NUMBER = sr.DOCUMENT_NUMBER
LEFT JOIN data_ext.asr_evc evc
  ON asr.document_number = evc.document_number
  AND (evc.uref = '01' OR evc.uref IS NULL)
LEFT JOIN data_ext.asr_evc evc2
  ON asr.document_number = evc2.document_number
  AND (evc2.uref = '02' OR evc2.uref IS NULL)
LEFT JOIN access_billing_circuit_data abcd2
  ON COALESCE(
       Regexp_replace(evc.RUID, '[^a-zA-Z0-9]', ''), 
       Regexp_replace(evc2.RUID, '[^a-zA-Z0-9]', '')
     ) = Regexp_replace(abcd2.exchange_carrier_circuit_id, '[^a-zA-Z0-9]', '')
LEFT JOIN access_service_request asr2 
  ON abcd2.document_number = asr2.document_number;</property>
